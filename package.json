{
  "name": "ezku.github.io",
  "repository": "git@github.com:Ezku/ezku.github.io.git",
  "author": "Eevert Saukkokoski <eevert.saukkokoski@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "yarn parcel serve ./v2/index.html",
    "build": "yarn build:parcel && yarn build:prerender",
    "build:parcel": "yarn parcel build ./v2/index.html",
    "build:prerender": "yarn prerender --renderer=jsdom --source=dist --target=dist --renderAfterDocumentEvent=react-render-complete index.html"
  },
  "dependencies": {
    "@types/react": "^16.7.13",
    "@types/react-dom": "^16.0.11",
    "normalize.css": "^8.0.1",
    "prerenderer-cli": "https://github.com/Ezku/prerenderer-cli#v0.0.2",
    "prettier": "^1.15.3",
    "react": "^16.6.3",
    "react-dom": "^16.6.3"
  },
  "devDependencies": {
    "parcel": "^1.10.3",
    "typescript": "^3.2.2"
  }
}
